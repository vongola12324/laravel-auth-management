{
    "name": "vongola12324/laravel-auth-management",
    "description": "An advanced auth management mechanism.",
    "keywords": [
        "laravel", "auth", "management"
    ],
    "homepage": "https://github.com/vongola12324/laravel-auth-management",
    "type": "library",
    "authors": [
        {
            "name": "Vongola",
            "email": "vongola12324@coder.tw",
            "role": "Developer"
        },
        {
            "name": "KID",
            "email": "jyhsu2000@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0"
    },
    "autoload": {
        "psr-4": {
            "Vongola\\Auth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vongola\\AuthTest\\": "test/"
        }
    },
    "license": "MIT",
    "minimum-stability": "stable",
    "extra": {
        "component": "package",
        "laravel": {
            "providers": [
                "Vongola\\Auth\\Providers\\AuthManagementServiceProvider"
            ],
            "aliases": {
                "AuthManager" => "Vongola\\Auth\\Facades\\AuthManagementFacade";
            }
        }
    }
}
